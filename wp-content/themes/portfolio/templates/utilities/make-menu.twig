{% if items %}
	<ul class="{{
		'primary' == location ? 'navbar-nav'  :
		'footer' == location ? 'nav d-flex justify-content-end' :
		'nav'
	}}">
	{% for item in items %}
		<li class="nav-item {{ item.classes | join(' ') }}">
			<a class="nav-link" target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
			{% include "utilities/make-menu.twig" with {'items': item.children} %}
		</li>
	{% endfor %}
	</ul>
{% endif %}
